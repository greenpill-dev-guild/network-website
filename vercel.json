{
  "buildCommand": "npm run build",
  "outputDirectory": "./",
  "installCommand": "npm install",
  "functions": {},
  "headers": [
    {
      "source": "/pdf/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/pdf"
        }
      ]
    },
    {
      "source": "/src/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
} 